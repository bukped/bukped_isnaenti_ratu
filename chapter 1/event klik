  <button onclick="hay()">Hay selamat datang</button>
  <button id="hay - selamat datang ">Hello selamat datang</button>

  <script>
    function hello(){
      alert("Hello World");
    }

    const btnLogin = document.getElementById("hay-selamat datang");
    btnLogin.addEventListener("click", function(){
      alert("hay selamat datang");
    });
  </script>
